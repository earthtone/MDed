{
  "name": "MDed",
  "version": "1.0.0",
  "description": "Markdown Editor",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf .temp",
    "pretest": "eslint ./**/*.js",
    "test": "browserify app/test/*.spec.js | tape-run | tap-spec",
    "prestart": "mkdir -p .temp",
    "start": "electron .",
    "build": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds"
  },
  "author": "earthtone <earthlyillusion@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^15.2.0",
    "electron": "^1.8.7",
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^12.1.0",
    "eslint": "^4.19.1",
    "parallelshell": "^3.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-run": "^3.0.0"
  },
  "dependencies": {
    "marked": "^0.3.6",
    "normalize-css": "^2.3.1",
    "w3-css": "^4.0.4"
  }
}
